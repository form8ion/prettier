{
  "name": "@form8ion/prettier",
  "description": "form8ion plugin for projects that manage code style with prettier",
  "license": "MIT",
  "type": "commonjs",
  "repository": "form8ion/prettier",
  "bugs": "https://github.com/form8ion/prettier/issues",
  "homepage": "https://npm.im/@form8ion/prettier",
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org)",
  "scripts": {
    "build:js": "rollup --config",
    "watch": "run-s 'build:js -- --watch'",
    "clean": "rimraf ./lib",
    "prebuild": "run-s clean",
    "build": "npm-run-all --print-label --parallel build:*",
    "prepack": "run-s build",
    "pretest:integration:base": "run-s build",
    "lint:gherkin": "gherkin-lint",
    "test:integration": "run-s 'test:integration:base -- --profile noWip'",
    "test:integration:base": "NODE_OPTIONS=--enable-source-maps DEBUG=any cucumber-js test/integration",
    "test:integration:debug": "DEBUG=test run-s test:integration",
    "test:integration:wip": "run-s 'test:integration:base -- --profile wip'",
    "test:integration:wip:debug": "DEBUG=test run-s 'test:integration:wip'",
    "test:integration:focus": "run-s 'test:integration:base -- --profile focus'",
    "test:unit": "cross-env NODE_ENV=test c8 run-s test:unit:base",
    "test:unit:base": "DEBUG=any mocha 'src/**/*-test.js'",
    "lint:lockfile": "lockfile-lint",
    "lint:sensitive": "ban",
    "prepare": "husky install",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "run-s lint:js -- --fix",
    "lint:md": "remark . --frail",
    "generate:md": "remark . --output",
    "pregenerate:md": "run-s build",
    "lint:peer": "npm ls >/dev/null",
    "test": "npm-run-all --print-label build --parallel lint:* --parallel test:*",
    "lint:engines": "ls-engines"
  },
  "files": [
    "example.js",
    "lib/"
  ],
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "runkitExampleFilename": "./example.js",
  "main": "./lib/index.js",
  "module": "./lib/index.mjs",
  "exports": {
    "require": "./lib/index.js",
    "import": "./lib/index.mjs"
  },
  "version": "0.0.0-semantically-released",
  "engines": {
    "node": "^14.18 || >=16"
  },
  "devDependencies": {
    "@babel/register": "7.18.9",
    "@cucumber/cucumber": "8.5.0",
    "@form8ion/babel-preset": "1.6.93",
    "@form8ion/commitlint-config": "1.0.36",
    "@form8ion/eslint-config": "5.0.7",
    "@form8ion/eslint-config-cucumber": "1.4.1",
    "@form8ion/eslint-config-mocha": "2.0.3",
    "@form8ion/remark-lint-preset": "4.0.1",
    "@rollup/plugin-babel": "5.3.1",
    "@travi/any": "2.1.5",
    "ban-sensitive-files": "1.9.18",
    "c8": "7.12.0",
    "chai": "4.3.6",
    "cross-env": "7.0.3",
    "cz-conventional-changelog": "3.3.0",
    "gherkin-lint": "4.2.2",
    "husky": "8.0.1",
    "lockfile-lint": "4.7.6",
    "ls-engines": "0.7.0",
    "mocha": "10.0.0",
    "mock-fs": "5.1.2",
    "npm-run-all": "4.1.5",
    "remark-cli": "11.0.0",
    "remark-toc": "8.0.1",
    "remark-usage": "10.0.1",
    "rimraf": "3.0.2",
    "rollup": "2.77.0",
    "rollup-plugin-auto-external": "2.0.0",
    "sinon": "14.0.0"
  },
  "dependencies": {
    "@form8ion/config-file": "^1.0.1",
    "@form8ion/core": "^1.8.0"
  }
}
